{
  "name": "calcite-web",
  "version": "1.0.3",
  "description": "SASS & CSS Framework for Esri websites",
  "private": true,
  "homepage": "https://github.com/esri/calcite-web",
  "scripts": {
    "test": "semistandard",
    "start": "run-p sass *:watch",
    "deploy": "run-s rollup sass acetate gh-pages",
    "predist": "rimraf dist/",
    "dist": "run-s sass:dist rollup minify",
    "prerelease": "node bin/s3.js",
    "release": "gh-release",
    "acetate": "node bin/server.js",
    "acetate:watch": "node bin/server.js --watch",
    "gh-pages": "gh-pages -d docs/build -o upstream",
    "rollup": "rollup -c lib/js/rollup.config.js",
    "rollup:watch": "rollup -c lib/js/rollup.config.js -w",
    "sass": "node-sass docs/source/assets/css/ --include-path lib/sass -q --source-map-embed -o docs/build/assets/css/",
    "sass:watch": "npm run sass -- -w",
    "sass:dist": "node-sass lib/sass/ -q -o dist/css/",
    "minify": "node bin/minify.js"
  },
  "semistandard": {
    "ignore": [
      "bin/",
      "docs/",
      "dist/"
    ]
  },
  "main": "dist/js/calcite-web.js",
  "js-next:main": "es6.js",
  "module": "es6.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/esri/calcite-web.git"
  },
  "author": {
    "name": "Esri Portland R&D Center",
    "email": "<portland@esri.com>",
    "url": "http://pdx.esri.com/"
  },
  "contributors": [
    {
      "name": "Paul C. Pederson",
      "email": "ppederson@esri.com"
    },
    {
      "name": "Nikolas Wise",
      "email": "nwise@esri.com"
    }
  ],
  "files": [
    "dist/",
    "es6.js",
    "lib/js/"
  ],
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/esri/calcite-web/issues"
  },
  "devDependencies": {
    "acetate": "^2.0.2",
    "acetate-cli": "^1.0.1",
    "cssstats": "^3.2.0",
    "esri-global-nav": "github:Esri/global-nav#v1.0.6",
    "fs-extra": "^7.0.0",
    "gh-pages": "^0.11.0",
    "gh-release": "^3.3.0",
    "ncp": "^2.0.0",
    "node-sass": "paulcpederson/node-sass#watch-directory",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.5",
    "rollup-plugin-babel-minify": "^4.0.0",
    "rollup-plugin-buble": "^0.15.0",
    "s3": "^4.4.0",
    "scss-to-json": "^2.0.0",
    "semistandard": "*",
    "sqwish": "^0.2.2"
  },
  "dependencies": {
    "@esri/calcite-ui-icons": "^1.2.0"
  }
}
